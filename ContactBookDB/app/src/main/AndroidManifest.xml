<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.murager.com.contactbookdb" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.activies.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.activies.AddContact"
            android:label="@string/title_activity_add_contact" >
        </activity>
        <activity
            android:name=".activities.activies.PersonContact"
            android:label="@string/title_activity_person_contact" >
        </activity>
    </application>

</manifest>
